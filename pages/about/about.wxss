/* pages/about/about.wxss */
page {
	width: 100%;
	height: 100%;
}
.about {
	width: 100%;
	height: 100%;
	background-color: #fff;
	text-align: center;
	overflow: hidden;
}
.about_logo {
	width: 120rpx;
	height: 120rpx;
	display: block;
	margin: 100rpx auto 10rpx;
}
.slogan {
	font-size: 26rpx;
	color: #333;
}
.version {
	font-size: 26rpx;
	color: #999;
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	position: fixed;
	bottom: 30rpx;
	left: 0;
}